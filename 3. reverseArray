class Solution {
    public void reverseArray(int arr[]) {
        
        for(int i=0, j=arr.length-1; i<=j; i++, j--) {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
    }
}

class Solution {
    public void reverseArray(int arr[]) {
        
        Collections.reverse(arr);
        }
    }
}

class Solution {
    public void reverseArray(int arr[]) {
        int n = arr.length;
        for(int i=0, j=n/2; i<=j; i++, j--) {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
    }
}

class Solution {
    static void reverseArray(int[] arr) {
        int n = arr.length;
        
        for (int i = 0; i < n / 2; i++) {
            int temp = arr[i];
            arr[i] = arr[n - i - 1];
            arr[n - i - 1] = temp;
        }
    }
}
